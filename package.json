    "axios": "^0.9.1",
    "es6-promise": "^3.1.2",
    "minimist": "^1.2.0",
    "moment": "^2.11.2",
    "x2js": "^2.0.0",
    "xmldom": "^0.1.22"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "uglifyify": "^3.0.1"
  }
